{
  "name": "trackit-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "deploy:dev": "npm run type-check && npm run build:dev && npx firebase-tools use dev && npx firebase-tools deploy --only hosting",
    "deploy:prod": "npm run type-check && npm run build && npx firebase-tools use prod && npx firebase-tools deploy --only hosting",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "upgrade-lock": "npx shx rm -rf package-lock.json node_modules && npm i --package-lock"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.11.0"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.8.2",
    "@mdi/font": "^7.2.96",
    "@vueform/slider": "^2.1.7",
    "@vueform/toggle": "^2.1.3",
    "export-from-json": "^1.7.2",
    "firebase": "^9.18.0",
    "pinia": "^2.0.33",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.10"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "prettier": "^2.8.5",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-pwa": "^0.14.4",
    "vue-tsc": "^1.2.0"
  },
  "author": "Richard Lindner <rlindner81@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com:rlindner81/tracker.git"
  }
}
